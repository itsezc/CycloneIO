<?xml version='1.0'?>
<animation name="fx.154" desc="Massive Egg FX">
	<remove id="ha" />
	<remove id="he" />
	<remove id="hr" />
	<remove id="hrb" />
	<remove id="ey" />
	<remove id="fa" />
	<remove id="fc" />
	<remove id="hd" />
	<remove id="ca" />
	<remove id="cc" />
	<remove id="ch" />
	<remove id="wa" />
	<remove id="lh" />
	<remove id="ls" />
	<remove id="lc" />
	<remove id="li" />
	<remove id="rh" />	
	<remove id="rc" />
	<remove id="rs" /> 

	<add id="fx154_1" align="top" />
	<shadow id="std_fx154_sd_0" />

	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso">
			<item id="bd" base="fx154_1"/>
		</bodypart>
		<fx id="fx154_1" action="Default" frame="0" dz="-1"/>
	</frame>

	<override name="mv_massiveegg" override="mv">
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="0" />
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="1" />
		<bodypart id="bottom"/>
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="2" />
		<fx id="fx154_1" action="Default" frame="0" dy="-16" />
	</frame>
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="3" />
		<fx id="fx154_1" action="Default" frame="0" dy="-16" />
	</frame>
	</override>

	<override name="sign_massiveegg" override="sign">
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm" action="Sign"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="2" />
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm" action="Sign"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="3" />
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>	
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm" action="Sign"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="0" />
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>	
	<frame>
		<bodypart id="head"/>
		<bodypart id="leftarm" action="Sign"/>
		<bodypart id="rightarm"/>
		<bodypart id="torso" action="Move" frame="1" />
		<fx id="fx154_1" action="Default" frame="0" dy="-15" />
	</frame>
	</override>

   	<override name="sit_massiveegg" override="sit">
	<frame>
		<bodypart id="torso" action="Sit" frame="0" />
		<fx id="fx154_1" action="Default" frame="0" dy="-28" dz="-1" />
	</frame>
	</override>
</animation>
